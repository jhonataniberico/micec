@media (max-width:600px) {
    mat-card-subtitle {
        display: none !important;
    }
    .cabeceraBusqueda {
        justify-content: center!important;
    }
    .mt-x {
        margin-top: 0px!important;
    }
    .centrarDoctor {
        display: flex;
        justify-content: center;
    }
    .centrarCPI {
        text-align: center;
    }
    .contEspecialidad {
        text-align: center;
    }
    .btnRegresar {
        width: 50%;
        margin: auto !important;
    }
    .example-header-image {
        width: 150px;
        height: 150px;
        margin: auto;
    }
    .letraAzul {
